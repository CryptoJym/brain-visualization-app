# Brain Visualization Environment Variables
VITE_APP_TITLE=Brain Visualization
VITE_PUBLIC_URL=https://brain-visualization.vercel.app

# Anthropic AI Configuration (for Conversational Assessment)
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key

# Mem0 AI Configuration (SOC 2 & HIPAA Compliant)
VITE_MEM0_API_KEY=your_mem0_api_key
VITE_MEM0_ORG_ID=your_organization_id_optional
VITE_MEM0_PROJECT_ID=brain-assessments

# OpenRouter API Configuration (for AI-Generated Brain Visualizations)
VITE_OPENROUTER_API_KEY=your_openrouter_api_key
# Image generation models supported through OpenRouter:
# - openai/dall-e-3 (recommended for high quality medical illustrations)
# - stability-ai/stable-diffusion-xl
# Note: google/gemini-2.5-flash-image-preview is NOT an image generator
VITE_IMAGE_MODEL=openai/dall-e-3

# Security Settings
VITE_ENABLE_AUTH=true
VITE_SESSION_DURATION=3600